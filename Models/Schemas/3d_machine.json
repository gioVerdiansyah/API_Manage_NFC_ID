{
  "$jsonSchema":{
    "bsonType": "object",
    "required": ["machine_name"],
    "properties":{
      "nfc_id": {
        "bsonType": "string",
        "description": "NFC ID Machine"
      },
      "machine_name": {
        "bsonType": "string",
        "description": "Name 3D machine"
      },
      "isUsed": {
        "bsonType": "bool",
        "description": "Is the machine used another device?"
      },
      "latest_used": {
        "bsonType": ["date", "null"],
        "description": "Latest machine used"
      },
      "total_used": {
        "bsonType": "int",
        "description": "Total machine used"
      }
    }
  }
}